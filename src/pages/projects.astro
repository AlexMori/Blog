---
import BaseLayout from "../layouts/BaseLayout.astro";
import HorizontalCard from "../components/HorizontalCard.astro";
---

<BaseLayout title = "Projects" sideBarActiveItemID="projects">
  <div>
    <div class="text-3xl w-full font-bold mb-5">Projects Header</div>
  </div>

  <HorizontalCard
    title="Champion info"
    img="/ugg.png"
    desc="Serverless lambda that retrieves champion information from u.gg with puppeteer.js"
    url="https://github.com/AlexMori/champion-info"
    badge="NEW"
  />

</BaseLayout>
